<% layout("layouts/boilerplate") %>
<!-- copied from btstrap alert  -->
<div class="row">
  <div class="col-6 offset-3"></div>
  <div class="alert alert-danger" role="alert">
    <h4 class="alert-heading"><%= err.message %></h4>
    <hr />
    <p class="mb-0"><%= err.statusCode %></p>
    <p><%= err.stack %></p>
  </div>
</div>
